# the love of my life
#  complete with single tab
set show-all-if-ambiguous on

$if Bash
    DEL: ""
    RETURN: "\C-l\C-j"
    "\C-q":"\"\"\C-b"
    "\C-t": "\C-aecho \C-e" 
    "\C-x*": glob-expand-word
    "\C-z": forward-word
    "\ef": "\C-z\C-f"
    "\eh": backward-kill-word
    "\ej": "\C-v\C-j"
$endif
