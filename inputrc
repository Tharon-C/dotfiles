set show-all-if-ambiguous on

$if Bash
    "!!":"\C-e`\C-avim `\C-b"
    "\C-q":"\"\"\C-b"
    "\C-t": "\C-aecho \C-e" 
    "\C-x*": glob-expand-word
    "\C-z": forward-word
    "\ef": "\C-z\C-f"
    "\eh": backward-kill-word
    "\ej": "\C-v\C-j"
    RET: " # did you mean ^J? "
$endif
