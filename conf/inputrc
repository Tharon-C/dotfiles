$if Bash
    "!!":"\C-e`\C-avim `\C-b"
    "@@":"\C-p\C-e | less\C-j"
    "\C-q":"\"\"\C-b"
    "\C-t": "\C-aecho \C-e" 
    "\C-x*": glob-expand-word
    "\C-z": forward-word
    "\ef": "\C-z\C-f"
    "\eh": backward-kill-word
    "\ej": "\C-v\C-j"
